{
  "name": "maoxian-web-clipper",
  "version": "0.1.60",
  "description": "A web extension to clip information from web page. Save it to your local machine to avoid information invalidation. Not bored registration, Not charged.",
  "main": "",
  "scripts": {
    "build-chrome": "cross-env NODE_ENV=production MX_PLATFORM=chrome MX_PLATFORM_ID=$MX_CHROME_ID webpack --mode=production --progress",
    "build-firefox": "cross-env NODE_ENV=production MX_PLATFORM=firefox MX_PLATFORM_ID=$MX_FIREFOX_ID webpack --mode=production --progress",
    "build-all": "npm run build-chrome && npm run build-firefox",
    "build-test": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "watch": "cross-env NODE_ENV=development webpack --mode=development --watch --progress",
    "test": "mocha --recursive --require esm"
  },
  "author": "Mika",
  "license": "MIT",
  "devDependencies": {
    "@altairwei/collect-loader": "^3.1.0",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "esm": "^3.2.25",
    "exports-loader": "^0.7.0",
    "extract-loader": "^5.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.14",
    "imports-loader": "^0.8.0",
    "inert-entry-webpack-plugin": "^4.0.2",
    "interpolate-loader": "^2.0.1",
    "loader-runner": "^3.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "jsdom": "^15.2.1",
    "mocha": "^7.1.0",
    "prop-loader": "^1.0.0",
    "raw-loader": "^4.0.0",
    "ref-loader": "^1.0.3",
    "remove-files-webpack-plugin": "^1.4.0",
    "sinon-chrome": "^3.0.1",
    "spawn-loader": "^6.0.0",
    "umd-compat-loader": "^2.1.2",
    "val-loader": "^2.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-extension-reloader": "^1.1.4",
    "worker-loader": "^2.0.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "awesomplete": "^1.1.5",
    "blueimp-md5": "^2.12.0",
    "bootstrap": "^4.5.2",
    "css-selector-generator": "^2.1.0",
    "pikaday": "^1.8.0",
    "roddeh-i18n": "^1.2.0",
    "strip-css-comments": "^4.1.0",
    "turndown": "^6.0.0",
    "turndown-plugin-gfm": "^1.0.2",
    "webextension-polyfill": "^0.6.0"
  }
}
